digraph srr {
{node [ shape = record ] p y z};
{node [ shape = triangle ] T1 T2 T3 T4};
p [ label ="<f0> | <f1> p | <f2> ", style=filled ];
y [ label ="<f0> | <f1> y | <f2> "];
